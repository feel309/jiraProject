<body>

  <div class="container">
      <div>Register an Account</div>
      
       <!-- 이슈 등록 -->
        <form action="/issue/enroll" method="post" enctype="multipart/form-data">
			<div>
				<label for="inputUserid">Summary</label> <input type="text"
					name="summary" id="summary" class="form-control"
					required="required" autofocus="autofocus">
			</div>
			<div>
				<label for="inputPassword">Description</label> <input type="text"
					name="description" id="description" class="form-control"
					required="required">
			</div>
			<div>
                  <label for="status">Status</label>
                  <input type="text" name="status" id="status" class="form-control" required="required">
                  <span id="status"></span>
             </div>

          <div class="form-group">
            <div class="form-label-group">
              <label for="inputName">Attachments</label>
              <input type="file" name="ori_name" id="ori_name" class="form-control" required="required"><br/>
            </div>
          </div>

          
          <input class="btn btn-primary btn-block" type="submit" value="Register" id="submit">
          <input class="btn btn-primary btn-block" type="reset" value="Cancel">
        </form>
        
        

  </div>
	
	
	
 
  
